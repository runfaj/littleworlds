        <!-- permission popup -->
        <string name="ads_permission_title">Borders App Usage</string>
        <string name="ads_permission_msg">Hi! This app requires a few special permissions to function properly. In order to proceed, you\'ll need to accept the following permission items:\n\n&#8212; Access Location\n\u0020\u0020\u0020\u0020(For Notifications)\n&#8212; Read Phone State\n\u0020\u0020\u0020\u0020(For Notifications)\n&#8212; Get Accounts\n\u0020\u0020\u0020\u0020(For free version ads)\n&#8212; Write External Storage\n\u0020\u0020\u0020\u0020(For Theme Customizing and Sharing)\n&#8212; Receive Boot Completed\n\u0020\u0020\u0020\u0020(For Themes to Show After Restart)\n&#8212; System Alert Window\n\u0020\u0020\u0020\u0020(For Drawing the Themes)\n\nTo proceed using the app, please accept these permissions on the next screen!</string>
        <string name="ads_permission_yes">Okay!</string>
        <string name="ads_permission_no">Nope :(</string>
        <string name="overlay_permission_title">Required Permission</string>
        <string name="overlay_permission_msg">This app\'s purpose is to allow you to have persistent themes on all screens. This requires a special overlay permission to be enabled in order to work. Please click "okay", then enable the permission on the next screen.</string>
        <string name="overlay_permission_yes">Okay</string>

